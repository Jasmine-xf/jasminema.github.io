[32m+[m[32m// Â∞ÜÁôΩËâ≤ËÉåÊôØËΩ¨Êç¢‰∏∫ÈÄèÊòé[m
[32m+[m[32mfunction initBackgroundRemoval() {[m
[32m+[m[32m    const targetImages = document.querySelectorAll('.gallery-item img[src*="indoor web low fi"]');[m
[32m+[m[32m    targetImages.forEach(img => {[m
[32m+[m[32m        if (img.dataset.bgProcessed === 'true') return;[m
[32m+[m[32m        const processImage = () => {[m
[32m+[m[32m            removeWhiteBackground(img);[m
[32m+[m[32m            img.dataset.bgProcessed = 'true';[m
[32m+[m[32m        };[m
[32m+[m[32m        if (img.complete && img.naturalWidth) {[m
[32m+[m[32m            processImage();[m
[32m+[m[32m        } else {[m
[32m+[m[32m            img.addEventListener('load', processImage, { once: true });[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction removeWhiteBackground(imageElement) {[m
[32m+[m[32m    const canvas = document.createElement('canvas');[m
[32m+[m[32m    const context = canvas.getContext('2d', { willReadFrequently: true });[m
[32m+[m[32m    const { naturalWidth, naturalHeight } = imageElement;[m
[32m+[m
[32m+[m[32m    if (!naturalWidth || !naturalHeight) return;[m
[32m+[m
[32m+[m[32m    canvas.width = naturalWidth;[m
[32m+[m[32m    canvas.height = naturalHeight;[m
[32m+[m[32m    context.drawImage(imageElement, 0, 0, naturalWidth, naturalHeight);[m
[32m+[m
[32m+[m[32m    const imageData = context.getImageData(0, 0, naturalWidth, naturalHeight);[m
[32m+[m[32m    const { data } = imageData;[m
[32m+[m[32m    const tolerance = 240; // ÈòàÂÄºË∂ä‰ΩéÔºå‰øùÁïôÁöÑÊµÖËâ≤Ë∂äÂ§ö[m
[32m+[m
[32m+[m[32m    for (let i = 0; i < data.length; i += 4) {[m
[32m+[m[32m        const r = data[i];[m
[32m+[m[32m        const g = data[i + 1];[m
[32m+[m[32m        const b = data[i + 2];[m
[32m+[m
[32m+[m[32m        if (r > tolerance && g > tolerance && b > tolerance) {[m
[32m+[m[32m            data[i + 3] = 0;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    context.putImageData(imageData, 0, 0);[m
[32m+[m[32m    imageElement.src = canvas.toDataURL('image/png');[m
[32m+[m[32m    imageElement.classList.add('bg-removed');[m
[32m+[m[32m}[m
